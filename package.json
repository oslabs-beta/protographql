{
  "name": "protographql",
  "version": "1.0.0",
  "description": "ProtGraphQl is prototyping tool to easily build and visualize GraphQl schemas and queries",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack -p",
    "dev": "webpack-dev-server --mode=development",
    "dist": "build",
    "package-all": "build && electron-builder build -mwl",
    "test": "jest"
  },
  "build": {
    "productName": "ProtoGraphQL",
    "appId": "org.oslabs.Protographql.todo",
    "asar": false,
    "files": [
      "public/",
      "index.html",
      "main.js",
      "apollo-server/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "public/assets/pictures/icon",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "oslabs-beta",
      "repo": "protographql",
      "private": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/protographql/protographql.git"
  },
  "author": {
    "name": "ProtoGraphQL",
    "email": "protographql-labs@todo.com",
    "url": "https://protographql.todo.org"
  },
  "contributors": [
    {
      "name": "Alena Budzko",
      "email": "#TODO",
      "url": "#TODO"
    },
    {
      "name": "Bryan Fong",
      "email": "",
      "url": ""
    },
    {
      "name": "Rodolfo Guzman",
      "email": "",
      "url": ""
    },
    {
      "name": "Jarred Jack-Harewood",
      "email": "",
      "url": ""
    },
    {
      "name": "Geoffrey Lin",
      "email": "",
      "url": ""
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/protographql/protographql/issues"
  },
  "homepage": "https://github.com/protographql/protographql#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "d3": "^5.9.2",
    "electron": "^9.1.0",
    "enzyme": "^3.9.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "styled-components": "^4.3.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/cli": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/polyfill": "^7.6.0",
    "@material-ui/icons": "^4.0.1",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-error": "^1.1.11",
    "apollo-server-express": "^2.9.0",
    "archiver": "^3.0.0",
    "babel-jest": "^24.9.0",
    "dotenv": "^8.1.0",
    "graphiql": "^0.14.2",
    "graphql": "^14.5.3",
    "graphql-tag": "^2.10.1",
    "graphql-transport-electron": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-draggable": "^3.3.0"
  }
}
